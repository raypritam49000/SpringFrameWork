<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.spring.core.model.Address" name="address">
       <property name="city" value="Ropar"/>
        <property name="state" value="Punjab"/>
    </bean>
    
    <bean class="com.spring.core.model.User" name="user">
      <property name="id" value="123"/>
      <property name="name" value="Pritam Ray" />
      <property name="salary" value="30000.0" />
       <property name="address" ref="address" />
    </bean>
</beans>
