<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">


    <context:annotation-config></context:annotation-config>
    
	<bean class="com.springcore.model.Address" name="address1">
		    <property name="city" value="Ropar"/>
			<property name="state" value="Punjab"/>
			<property name="country" value="India"/>
	</bean>
	
	<bean class="com.springcore.model.Address" name="address2">
		    <property name="city" value="Prem Nagar"/>
			<property name="state" value="Punjab"/>
			<property name="country" value="India"/>
	</bean>
	
	<bean class="com.springcore.model.Address" name="address3">
		    <property name="city" value="Asron"/>
			<property name="state" value="Punjab"/>
			<property name="country" value="India"/>
	</bean>
	
	
	<bean class="com.springcore.model.Employee" name="employee"/>
		    


</beans>